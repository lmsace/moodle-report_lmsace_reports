{{!
    @template report_lmsace_reports/courseblocks.

    Template for the course reports.

    Example context (json):
    {
        "enrolments" : "5",
        "activity_completions": "5",
        "course_completions" : "10",
        "newregistrations" : "3",
        "visits" : "20"
    }
}}
<div class="table-block">
    <table class="table">
        <tbody>
            <tr>
                <td>{{#str}}enrolment, report_lmsace_reports{{/str}}</td>
                <td>{{enrolments}}</td>
            </tr>
            <tr>
                <td>{{#str}}activitycompletion, report_lmsace_reports{{/str}}</td>
                <td> {{activity_completions}} </td>
            </tr>
            <tr>
                <td>{{#str}}coursecompletion, report_lmsace_reports{{/str}}</td>
                <td> {{course_completions}} </td>
            </tr>
            <tr>
                <td>{{#str}}newregistrations, report_lmsace_reports{{/str}}</td>
                <td> {{newregistrations}} </td>
            </tr>
            <tr>
                <td>{{#str}}visitstitle, report_lmsace_reports{{/str}}</td>
                <td> {{visits}} </td>
            </tr>
        </tbody>
    </table>
</div>
